{
  "name": "the-listening-lounge",
  "version": "0.0.0",
  "scripts": {
    "build": "ng build",
    "build:prod": "npm run clean && ng build -c production",
    "clean": "rimraf dist",
    "clean:modules": "rimraf node_modules",
    "deploy": "npm run build:prod && vercel --prod",
    "ng": "ng",
    "rebuild": "npm run clean && npm run clean:modules && npm install && npm run build",
    "start": "ng serve",
    "test": "ng test",
    "watch": "ng build --watch --configuration development"
  },
  "prettier": {
    "overrides": [
      {
        "files": "*.html",
        "options": {
          "parser": "angular"
        }
      }
    ]
  },
  "private": true,
